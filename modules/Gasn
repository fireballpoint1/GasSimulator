      # USE BORN BETHE ABOVE XION13(NION13) EV
      123 QION[13][I]=QIONC*0.001015
      124 CONTINUE
      if(EN &lt; (2.0*EION[13]:
      )) GO TO 125
      PEQION[13][I]=PEQEL[2][(I-IOFFION[13]))
      # CH2+
