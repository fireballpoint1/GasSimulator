      GO TO 358
      357 QIN[2][I]=0.203/(EIN[2]*BETA2)*(math.log(BETA2*GAMMA2*EMASS2/(4.0*EIN[2]))-BETA2-DEN[I]/2.0)*BBCONST*EN/(EN+EIN[2]+E[3])
      358 if(EN &lt;= (2.0*EIN[2])) GO TO 359
