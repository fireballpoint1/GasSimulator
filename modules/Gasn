      B=(XL1S[J-1]*YL1S[J]-XL1S[J]*YL1S[J-1])/(XL1S[J-1]-XL1S[J])
      QION[10][I]=(A*EN+B)*1.e-16
      PEQION[10][I]=PEQEL[2][(I-IOFFION[10]))
      #  K SHELL IONISATION
      2274 QION[11][I]=0.00
      PEQION[11][I]=0.50
      if(NANISO == 2):
      PEQION[11][I]=0.00
