      B=(XVIB[J-1]*YVIB5[J]-XVIB[J]*YVIB5[J-1])/(XVIB[J-1]-XVIB[J]) 
      QIN[54][I]=(A*EN+B)*1.e-16   
      GO TO 604
      # SCALE BY 1/E ABOVE XVIB[NVIB]
      603 QIN[54][I]=YVIB5(NVIB)*(XVIB[NVIB]/EN)*1.e-16
      604 if(EN &gt; 6.0) PEQIN[54][I]=PEQEL[2][(I-IOFFN[54])]                  
