      QIN[17][I]=.0016884/(EIN[17]*BETA2)*(math.log(BETA2*GAMMA2*EMASS2/(4.0*EIN[17]))-BETA2-DEN[I]/2.0)*BBCONST*EN/(EN+EIN[17]+BEF[1])
