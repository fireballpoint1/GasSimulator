  162 A=(YINPP[J]-YINPP[J-1])/(XINPP[J]-XINPP[J-1])                    
      B=(XINPP[J-1]*YINPP[J]-XINPP[J]*YINPP[J-1])/(XINPP[J-1]-XINPP[J])
      QION[7][I]=(A*EN+B)*1.e-16
      GO TO 164
