      #
      # 5 1P   SCALED FROM 4 1P   OSC STRENGTH  F=0.01504
      1040 if(EN &lt;= EIN[26]) GO TO 2000
      ER=EIN[26]/EIN[18]
      ENP=EN/ER
      if(ENP &gt; X41P(N41P):
      ) GO TO 1061
      DO 1050 J=2,N41P
      if(ENP &lt;= X41P[J]:
      ) GO TO 1060
      1050 CONTINUE
