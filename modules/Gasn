      EIN[62]=14.119
      EIN[63]=14.273
      EIN[64]=14.408
      EIN(65)=14.522
      EIN(66)=14.611
      EIN(67)=14.672
      # C3 PI 
      EIN(68)=11.779
      EIN(69)=13.100
      # A3 SIGMA
      EIN(70)=11.793
      EIN(71)=12.684
      # E3 SIGMA
      EIN(72)=13.253
      # EF1 SIGMA
      EIN(73)=12.301
      EIN(74)=12.841   
      # B#1 SIGMA BANDS
      EIN(75)=13.698
      EIN(76)=13.931
      EIN(77)=14.144
      EIN(78)=14.333
      EIN(79)=14.494
      EIN(80)=14.613
