      169 CONTINUE
      if(EN &lt; (2.0*EION[22]:
      )) GO TO 170
      PEQION[22][I]=PEQEL[2][(I-IOFFION[22])]
      # C3+
      170 if(EN &lt;= EION[23]) GO TO 175         
