            <span class="n">DISTOT</span><span class="o">=</span><span class="n">QSNGLSUM</span><span class="o">+</span><span class="n">QTRIPSUM</span><span class="o">+</span><span class="n">QIONSUM</span>               
            <span class="n">Q</span><span class="p">[</span><span class="mi">1</span><span class="p">][</span><span class="n">I</span><span class="p">]</span><span class="o">=</span><span class="n">Q</span><span class="p">[</span><span class="mi">2</span><span class="p">][</span><span class="n">I</span><span class="p">]</span><span class="o">+</span><span class="n">Q</span><span class="p">[</span><span class="mi">4</span><span class="p">][</span><span class="n">I</span><span class="p">]</span><span class="o">+</span><span class="n">VSUM</span><span class="o">+</span><span class="n">DISTOT</span>
            <span class="c">#     WRITE(6,444) EN,Q[1][I],Q[2][I],VSUM,QSNGLSUM,QTRIPSUM,QIONSUM,</span>
            <span class="c">#    /QIONG,DISTOT</span>
            <span class="c"># 444 print(' EN=',D12.6,' TOT=','%.4f' % ,' ELAS=','%.4f' % ,' VIB SUM=',</span>
            <span class="c">#    /'%.4f' % ,/,' NEUTRAL DIP. DISS=','%.4f' % ,' TRIPLET DISS=','%.4f' % ,/,</span>
            <span class="c">#    /' COUNT IONISATION=','%.4f' % ,/' GROSS IONISATION=','%.4f' % ,/,</span>
            <span class="c">#    /' DISTOT=','%.4f' % )</span>
      <span class="c">#  SAVE COMPUTE TIME </span>
      <span class="k">if</span><span class="p">(</span><span class="n">EFINAL</span> <span class="o">&gt;</span> <span class="mf">1000.</span><span class="p">):</span>
            <span class="n">NIN</span><span class="o">=</span><span class="mi">48</span>
      <span class="k">for</span> <span class="n">J</span> <span class="ow">in</span> <span class="nb">range</span><span class="p">(</span><span class="mi">11</span><span class="p">,</span><span class="mi">46</span><span class="p">):</span>
            <span class="k">if</span><span class="p">(</span><span class="n">EFINAL</span> <span class="o">&lt;=</span> <span class="n">EIN</span><span class="p">[</span><span class="n">J</span><span class="p">]):</span>
                  <span class="n">NIN</span><span class="o">=</span><span class="n">J</span><span class="o">-</span><span class="mi">1</span>
                  <span class="k">break</span>
            <span class="c"># endif</span>
      <span class="k">return</span>                                                            
      <span class="c"># end </span>
<span class="k">def</span> <span class="nf">GAS2</span><span class="p">(</span><span class="n">Q</span><span class="p">,</span><span class="n">QIN</span><span class="p">,</span><span class="n">NIN</span><span class="p">,</span><span class="n">E</span><span class="p">,</span><span class="n">EIN</span><span class="p">,</span><span class="n">NAME</span><span class="p">,</span><span class="n">VIRIAL</span><span class="p">,</span><span class="n">EOBY</span> <span class="p">,</span><span class="n">PEQEL</span><span class="p">,</span><span class="n">PEQIN</span><span class="p">,</span><span class="n">PENFRA</span><span class="p">,</span><span class="n">KEL</span><span class="p">,</span><span class="n">KIN</span><span class="p">,</span><span class="n">QION</span><span class="p">,</span><span class="n">PEQION</span><span class="p">,</span><span class="n">EION</span><span class="p">,</span><span class="n">NION</span><span class="p">,</span><span class="n">QATT</span><span class="p">,</span><span class="n">NATT</span><span class="p">,</span><span class="n">QNULL</span><span class="p">,</span><span class="n">NNULL</span><span class="p">,</span><span class="n">SCLN</span><span class="p">,</span><span class="n">NC0</span><span class="p">,</span><span class="n">EC0</span><span class="p">,</span><span class="n">WKLM</span><span class="p">,</span><span class="n">EFL</span><span class="p">,</span><span class="n">NG1</span><span class="p">,</span><span class="n">EG1</span><span class="p">,</span><span class="n">NG2</span><span class="p">,</span><span class="n">EG2</span><span class="p">,</span><span class="n">IZBR</span><span class="p">,</span><span class="n">LEGAS</span><span class="p">,</span><span class="n">ISHELL</span><span class="p">,</span><span class="n">IONMODEL</span><span class="p">,</span><span class="n">ESPLIT</span><span class="p">,</span><span class="n">SCRPT</span><span class="p">,</span><span class="n">SCRPTN</span><span class="p">):</span>
