)) GO TO 3300
      PEQION[3][I]=PEQEL[2][(I-IOFFION[3]])
# GROSS SUM OF IONISATION STATE =4+5+6
 3300 Q456=0.0
      if(EN &lt;= 106.35):
 GO TO 3350
      if(EN &gt; XIN4(NION4):
) GO TO 3349
      DO 3310 J=2,NION4
      if(EN &lt;= XIN4[J]:
) GO TO 3325
 3310 CONTINUE
      J=NION4
 3325 A=(YIN4[J]-YIN4[J-1])/(XIN4[J]-XIN4[J-1])                         
      B=(XIN4[J-1]*YIN4[J]-XIN4[J]*YIN4[J-1])/(XIN4[J-1]-XIN4[J])       
      Q456=(A*EN+B)*1.0D-16 
      Q456=Q456*4.0/3.0
      if(EN &lt;= 160.45):
 GO TO 3350
      if(EN &gt; XIN5(NION5):
) GO TO 3349
      DO 3377 J=2,NION5
      if(EN &lt;= XIN5[J]:
