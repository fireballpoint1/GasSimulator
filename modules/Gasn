      B=(XM1S[J-1]*YM1S[J]-XM1S[J]*YM1S[J-1])/(XM1S[J-1]-XM1S[J])
      QION[7][I]=(A*EN+B)*1.e-16
      PEQION[7][I]=PEQEL[2][(I-IOFFION[7]])
      # L3 SHELL IONISATION
      2265 QION[8][I]=0.00
      PEQION[8][I]=0.50
