      PEQION[3][I]=PEQEL[2][(I-IOFFION[3]])
      # C2H4+ 
      75 if(EN &lt;= EION[4]) GO TO 80         
      if(EN &gt; XION4(NION4):
      ) GO TO 78
      DO 76 J=2,NION4                                                  
      if(EN &lt;= XION4[J]:
      ) GO TO 77                                     
      76 CONTINUE                                                          
