      QTEMP=QION[4][I]+QION[5][I]+QION[6][I]+QION[7][I]+QION[8][I]+QION[9][I]+QION[10][I]+QION[11][I]+QION[12][I]
      if(Q[5][I]:
 == 0.0) :
       QCORR=1.0
      else:
