      GO TO 330
      321 QIN[84][I]=APOPGS*YVB7(NVIB7)*(XVB7(NVIB7)/EN)*1.e-16        
      330 CONTINUE                                                          
      # 8V1                                                                   
      QIN[85][I]=0.0                                                     
      if(EN &lt;= EIN(85):
      ) GO TO 360                                 
      if(EN &gt; XVB8(NVIB8):
