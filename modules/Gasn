      Q456=Q456+(A*EN+B)*1.0D-16*5.0/3.0
      if(EN &lt;= 227.2):
 GO TO 3350
      if(EN &gt; XIN6(NION6):
) GO TO 3349
      DO 3323 J=2,NION6
      if(EN &lt;= XIN6[J]:
) GO TO 3324
 3323 CONTINUE
      J=NION6
