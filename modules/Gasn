      <span class="c">#  VIBRATIONAL DEGENERACY</span>
      <span class="n">DEGV4</span><span class="o">=</span><span class="mf">3.0</span>
      <span class="n">DEGV3</span><span class="o">=</span><span class="mf">3.0</span>
      <span class="n">DEGV2</span><span class="o">=</span><span class="mf">2.0</span>
      <span class="n">DEGV1</span><span class="o">=</span><span class="mf">1.0</span>  
      <span class="c"># CALC VIB LEVEL POPULATIONS</span>
      <span class="n">APOPV2</span><span class="o">=</span><span class="n">DEGV2</span><span class="o">*</span><span class="n">math</span><span class="o">.</span><span class="n">exp</span><span class="p">(</span><span class="n">EIN</span><span class="p">[</span><span class="mi">1</span><span class="p">]</span><span class="o">/</span><span class="n">AKT</span><span class="p">)</span>
      <span class="n">APOPV4</span><span class="o">=</span><span class="n">DEGV4</span><span class="o">*</span><span class="n">math</span><span class="o">.</span><span class="n">exp</span><span class="p">(</span><span class="n">EIN</span><span class="p">[</span><span class="mi">3</span><span class="p">]</span><span class="o">/</span><span class="n">AKT</span><span class="p">)</span>
      <span class="n">APOPV1</span><span class="o">=</span><span class="n">DEGV1</span><span class="o">*</span><span class="n">math</span><span class="o">.</span><span class="n">exp</span><span class="p">(</span><span class="n">EIN</span><span class="p">[</span><span class="mi">5</span><span class="p">]</span><span class="o">/</span><span class="n">AKT</span><span class="p">)</span>
      <span class="n">APOPV3</span><span class="o">=</span><span class="n">DEGV3</span><span class="o">*</span><span class="n">math</span><span class="o">.</span><span class="n">exp</span><span class="p">(</span><span class="n">EIN</span><span class="p">[</span><span class="mi">7</span><span class="p">]</span><span class="o">/</span><span class="n">AKT</span><span class="p">)</span>
      <span class="n">APOPGS</span><span class="o">=</span><span class="mf">1.0</span>
      <span class="n">APOPSUM</span><span class="o">=</span><span class="n">APOPGS</span><span class="o">+</span><span class="n">APOPV2</span><span class="o">+</span><span class="n">APOPV4</span><span class="o">+</span><span class="n">APOPV1</span><span class="o">+</span><span class="n">APOPV3</span>
      <span class="n">APOPGS</span><span class="o">=</span><span class="mf">1.0</span><span class="o">/</span><span class="n">APOPSUM</span>
      <span class="n">APOPV2</span><span class="o">=</span><span class="n">APOPV2</span><span class="o">/</span><span class="n">APOPSUM</span>
      <span class="n">APOPV4</span><span class="o">=</span><span class="n">APOPV4</span><span class="o">/</span><span class="n">APOPSUM</span>
      <span class="n">APOPV1</span><span class="o">=</span><span class="n">APOPV1</span><span class="o">/</span><span class="n">APOPSUM</span>
      <span class="n">APOPV3</span><span class="o">=</span><span class="n">APOPV3</span><span class="o">/</span><span class="n">APOPSUM</span>
      <span class="c"># RENORMALISE GROUND STATE TO ALLOW FOR EXCITATION X-SEC FROM</span>
      <span class="c"># EXCITED VIBRATIONAL STATES (EXACT APPROX IF THE HOT TRANSITIONS HAVE</span>
