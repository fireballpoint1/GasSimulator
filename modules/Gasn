)) GO TO 400
      PEQIN[45][I]=PEQEL[2][(I-IOFFN[45])]
# 8S4 J=1 F=0.0023 
  400 if(EN &lt;= EIN[46]) GO TO 413 
      QIN[46][I]=0.0023/(EIN[46]*BETA2)*(math.log(BETA2*GAMMA2*EMASS2/(4.0*EIN[46]))-BETA2-DEN[I]/2.0)*BBCONST*EN/(EN+EIN[46]+E[3])
      if(QIN[46][I]:
