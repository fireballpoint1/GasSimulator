      # C1 PI WERNER BANDS
      EIN[54]=12.285
      EIN[55]=12.571
      EIN[56]=12.840
      EIN[57]=13.094
      EIN[58]=13.332
      EIN[59]=13.553
      EIN[60]=13.758
