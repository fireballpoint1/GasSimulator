      DIMENSION Q(6,20000),QIN(250,20000),E[6],EIN(250),PENFRA[3,250]   
      CHARACTER*50 SCRPT(300),SCRPTN[10] 
      CHARACTER*25 NAME                                                 
      NAME=' DUMMY         '     
      # --------------------------------------------------------------------- 
      #   DUMMY ROUTINE                       
      # --------------------------------------------------------------------- 
      NION=1
      NATT=1
      NIN=0
      NNULL=0 
      IONMODEL=0
      DO 1 J=1,6
      1 KEL[J]=0
      DO 2 J=1,NIN
      2 KIN[J]=0           
      E[1]=0.0                                                          
      E[2]=0.0                                                          
      E[3]=0.0                                                          
      E[4]=0.0                                                          
      E[5]=0.0                                                          
      E[6]=0.0 
      SCRPT[1]='                              '
      SCRPT[2]=' ELASTIC       DUMMY          '
      SCRPT[3]=' IONISATION    ELOSS=         '
      SCRPT[4]=' ATTACHMENT                   '
      SCRPT[5]='                              '
      SCRPT[6]='                              '
      EN=-ESTEP/2.0                                      
      DO 900 I=1,NSTEP                                               
      EN=EN+ESTEP                                                       
      Q[2][I]=0.0                                                        
      Q[3][I]=0.0                                                        
      Q[4][I]=0.0                                                        
      Q[1][I]=Q[2][I]+Q[3][I]+Q[4][I]  
      900 CONTINUE                                                          
      #  SAVE COMPUTE TIME
      return                                                            
      # end   
