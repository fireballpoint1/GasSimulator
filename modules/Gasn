)) GO TO 401
      PEQIN[46][I]=PEQEL[2][(I-IOFFN[46])]
# 10D5 J=1 F=0.0005 
  401 if(EN &lt;= EIN[47]) GO TO 413 
      QIN[47][I]=0.0005/(EIN[47]*BETA2)*(math.log(BETA2*GAMMA2*EMASS2/(4.0*EIN[47]))-BETA2-DEN[I]/2.0)*BBCONST*EN/(EN+EIN[47]+E[3])
