      B=(XV1V1[J-1]*YV1V1[J]-XV1V1[J]*YV1V1[J-1])/(XV1V1[J-1]-XV1V1[J])
      QIN[4][I]=A*EN+B
      GO TO 476
  475 QIN[4][I]=YV1V1(N1V1)*(XV1V1(N1V1)/EN)**2 
  476 QIN[4][I]=QIN[4][I]*APOPGS*1.e-16 
      if(KIN[4]:
 == 2) PEQIN[4][I]=PEQEL[2][(I-IOFFN[4]))]#
# V3 SUPERELASTIC  
  500 QIN[5][I]=0.0
      PEQIN[5][I]=0.50
      if(KIN[5]:
 == 2) PEQIN[5][I]=0.0
      if(EN &lt;= 0.0):
 GO TO 550
      if((EN+EIN[6]:
) &gt; XVBV3(N1V3)) GO TO 530
      DO 510 J=2,N1V3
