      607 A=(YEXC1[J]-YEXC1[J-1])/(XEXC1[J]-XEXC1[J-1])                     
      B=(XEXC1[J-1]*YEXC1[J]-XEXC1[J]*YEXC1[J-1])/(XEXC1[J-1]-XEXC1[J]) 
      QIN[55][I]=(A*EN+B)*1.e-16     
      GO TO 609   
